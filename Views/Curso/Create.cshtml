<h1>@ViewData["titulo"]</h1>
<h6>@ViewBag.DataHora</h6>


<div class="mb-3">
  <label for="Titulo do Curso" class="form-label">Titulo Interno</label>
  <input type="text" class="form-control" id="tituloInterno" placeholder="Titulo">
</div>
<div class="mb-3">
  <label for="tituloMarketing" class="form-label">Titulo Marketing</label>
  <input type="text" class="form-control" id="tituloMarketing" placeholder="Titulo de Marketing">
</div>
<div class="mb-3">
  <label for="descricao" class="form-label">Descrição</label>
  <textarea class="form-control" id="descricao" rows="3"></textarea>
</div>
<button type="button" class="btn btn-outline-dark">Cadastrar</button>